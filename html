<body>
    <div class="container">
        <header>https://maznehahsh</header>
        <h2>أدخل كلمة المرور</h2>
        <form onsubmit="return checkPassword()">
            <input type="password" id="password" placeholder="كلمة المرور">
            <input type="submit" value="إرسال">
        </form>
        <div id="message" class="message"></div>
    </div>

    <script>
        function checkPassword() {
            const correctPassword = '73737';
            const enteredPassword = document.getElementById('password').value;
            const message = document.getElementById('message');

            if (enteredPassword === correctPassword) {
                message.textContent = 'مرحبًا بك في موقع اس!';
                message.style.color = 'green';
            } else {
                message.textContent = 'كلمة المرور خاطئة، حاول مرة أخرى.';
                message.style.color = 'red';
            }

            return false; // لمنع إعادة تحميل الصفحة
        }
    </script>
</body>
